{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "id": "linux-dictionary",
    "version": "0.2",
    "name": "Shellscript Custom Dictionary Configuration",
    "description": "Custom dictionary definition configuration for Shellscript.",
